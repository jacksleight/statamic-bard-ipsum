{
    "name": "jacksleight/statamic-bard-ipsum",
    "autoload": {
        "psr-4": {
            "JackSleight\\StatamicBardIpsum\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Bard Ipsum",
            "description": "This Statamic addon adds a lorem ipsum filler text generator to the Bard fieldtype."
        },
        "laravel": {
            "providers": [
                "JackSleight\\StatamicBardIpsum\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^3.4 || ^4.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
